# $Id: Makefile.in,v 1.2 2004-08-06 20:26:15 oops Exp $
top_builddir	= ..
CC		= @CC@ -Wall
SHELL		= @SHELL@
LIBTOOL		= @LIBTOOL@
CFLAGS		= @CFLAGS@
CPPFLAGS	= -I. -I.. @CPPFLAGS@
DEFS		= -DHAVE_CONFIG_H -DSUPPORT_UTF8 -DLINK_SIZE=2 -DPOSIX_MALLOC_THRESHOLD=10 $(CPPFLAGS)
PCREOBJ		= maketables.lo get.lo study.lo pcre.lo
DESTDIR		=

COMPILE		= $(LIBTOOL) --mode=compile $(CC) $(CFLAGS) $(DEFS) -prefer-pic
RM		= @RM@ -f

all: $(PCREOBJ)

maketables.lo: maketables.c
	$(COMPILE) -o $@ -c $<

get.lo: get.c
	$(COMPILE) -o $@ -c $<

study.lo: study.c
	$(COMPILE) -o $@ -c $<

pcre.lo: pcre.c
	$(COMPILE) -prefer-pic -o $@ -c $<

clean:
	-$(RM) *.o *.a *.lo .libs/*
