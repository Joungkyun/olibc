# $Id: Makefile.in,v 1.4 2011-02-18 18:08:32 oops Exp $
top_builddir	= ..
CC		= @CC@
CFLAGS		=
DESTDIR		=
SHELL		= @SHELL@

PROG		= @PACKAGE_NAME@
VERSION		= @PACKAGE_VERSION@

LIBTOOL		= @LIBTOOL@
INSTALL		= @INSTALL@
LS		= @LS@
GZIP		= @GZIP@
MKDIR		= @MKDIR@ -p

prefix		= @prefix@
exec_prefix	= @exec_prefix@
mandir		= @mandir@

D_LIBTOOL	= $(LIBTOOL) --mode=install @INSTALL_DATA@

install:
	$(MKDIR) $(DESTDIR)$(mandir)/ko/man1
	for i in `$(LS) *.1`; do \
		if test "$i" = "convert_racecode.1"; then \
			continue; \
		fi; \
		$(D_LIBTOOL) $$i $(DESTDIR)$(mandir)/ko/man1/ ;\
	done
