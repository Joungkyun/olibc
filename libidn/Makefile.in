#$Id: Makefile.in,v 1.2 2004-02-04 05:33:15 oops Exp $
CC		= @CC@ -Wall
CFLAGS		= @CFLAGS@ -fpic
LDFLAGS		= @LIBS@ @LDFLAGS@
LIBS		= @LIBS@
OBJECT		= idna.o nfkc.o profiles.o punycode.o stringprep.o rfc3454.o toutf8.o
CPPFLAGS	= -I. -I.. @CPPFLAGS@
DEFS		= @DEFS@ $(CPPFLAGS)

RM		= @RM@ -f

all: $(OBJECT)

idna.o: idna.c stringprep.h punycode.h idna.h
	$(CC) $(DEFS) $(CFLAGS) -c idna.c

nfkc.o: nfkc.c stringprep.h punycode.h idna.h gunidecomp.h gunicomp.h
	$(CC) $(DEFS) $(CFLAGS) -c nfkc.c

profiles.o: profiles.c stringprep.h punycode.h idna.h
	$(CC) $(DEFS) $(CFLAGS) -c profiles.c

punycode.o: punycode.c stringprep.h punycode.h idna.h
	$(CC) $(DEFS) $(CFLAGS) -c punycode.c

stringprep.o: stringprep.c stringprep.h punycode.h idna.h
	$(CC) $(DEFS) $(CFLAGS) -c stringprep.c

rfc3454.o: rfc3454.c stringprep.h
	$(CC) $(DEFS) $(CFLAGS) -c rfc3454.c

toutf8.o: toutf8.c stringprep.h punycode.h idna.h
	$(CC) $(DEFS) $(CFLAGS) -c toutf8.c

clean:
	-$(RM) -f *.o *~
